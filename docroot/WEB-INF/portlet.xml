<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>main</portlet-name>
		<display-name>Main Portlet</display-name>
		<portlet-class>com.inikah.portlet.MainPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/profile/view.jsp</value>
		</init-param>
		<init-param>
		    <name>add-process-action-success-action</name>
		    <value>false</value>
		</init-param>		
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Main Portlet</title>
			<short-title>Main Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>register</portlet-name>
		<display-name>Register</display-name>
		<portlet-class>
			com.liferay.util.bridges.mvc.MVCPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/register/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Register</title>
			<short-title>Register</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>invite</portlet-name>
		<display-name>Invite Portlet</display-name>
		<portlet-class>com.inikah.invite.InvitePortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/invite/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/invite/Language</resource-bundle>
		<portlet-info>
			<title>Invite Portlet</title>
			<short-title>Invite Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>payment</portlet-name>
		<display-name>Payment Portlet</display-name>
		<portlet-class>com.inikah.payment.PaymentPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/payment/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/payment/Language</resource-bundle>
		<portlet-info>
			<title>Payment Portlet</title>
			<short-title>Payment Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>filter</portlet-name>
		<display-name>Filter</display-name>
		<portlet-class>com.inikah.filter.Filter</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/filter/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Filter</title>
			<short-title>Filter</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-publishing-event xmlns:x='http://liferay.com'>
			<qname>x:filter</qname>
		</supported-publishing-event>
	</portlet>
	<portlet>
		<portlet-name>matches</portlet-name>
		<display-name>Matches</display-name>
		<portlet-class>com.inikah.matches.Matches</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/matches/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Matches</title>
			<short-title>Matches</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-processing-event xmlns:x='http://liferay.com'>
            <qname>x:filter</qname>
        </supported-processing-event>
	</portlet>
	<event-definition xmlns:x='http://liferay.com'>
		<qname>x:filter</qname>
	<value-type>com.inikah.slayer.MatchCriteria</value-type>
</event-definition>
</portlet-app>