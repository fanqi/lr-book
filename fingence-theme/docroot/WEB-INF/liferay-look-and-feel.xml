<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="fingence" name="Fingence">
		<settings>
			<setting key="portlet-setup-show-borders-default" value="value"/>
		</settings>
		
		<layout-templates>
			<custom>
				<layout-template id="fingence" name="Fingence">
					<template-path>/layout/report/fingence.tpl</template-path>
					<wap-template-path>/layout/report/fingence.wap.tpl</wap-template-path>
					<thumbnail-path>/layout/report/fingence.png</thumbnail-path>
				</layout-template>
				
				<layout-template id="investor" name="Investor">
					<template-path>/layout/investor/fingence.tpl</template-path>
					<wap-template-path>/layout/investor/fingence.wap.tpl</wap-template-path>
					<thumbnail-path>/layout/investor/fingence.png</thumbnail-path>					
				</layout-template>
			</custom>
		</layout-templates>
	</theme> 
</look-and-feel>